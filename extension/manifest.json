{
  "manifest_version": 2,
  "name": "Score Guessing Game",
  "description": "Guess your test score before you see it!",
  "version": "1.0",
  "content_scripts":[
    {
      "matches":[
        "https://studentscores.collegeboard.org/viewscore"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png",
    "256": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "high_low_game.html",
    "content.css",
    "icon.png"
  ]
}