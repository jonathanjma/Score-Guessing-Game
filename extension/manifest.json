{
  "manifest_version": 2,
  "name": "High Low Game",
  "description": "Guess your score before you see it!",
  "version": "1.0",
  "content_scripts":[
    {
      "matches":[
        "https://studentscores.collegeboard.org/viewscore"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ],
  "web_accessible_resources": [
    "high_low_game.html",
    "content.css",
    "icon.png"
  ]
}